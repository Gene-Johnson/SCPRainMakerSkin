;=============================================================================;
;------------------------------- SCP Desktop System---------------------------;
;=============================================================================;

[CaConsolasummy]
Measure=Calc
Formula=1
UpdateDivider=-1
OnUpdateAction=[!HideMeter MeterChannelInputLabel][!SetWallpaper "#@#\images\wallpaper.png" ]

[MeasureHotkeyRefresh]
Measure=Plugin
Plugin=HotKey
HotKey=CTRL R
KeyDownAction=!RefreshApp
DynamicVariables=1

;This Measure produces the random variables that decide the wait time between changes in fade
[MeasureCathodRayTubeRandomFade]
Measure=Calc
Formula=Random
LowBound=10
HighBound=100
UpdateRandom=1
DynamicVariables=1
UpdateDivider=1

[Measure_Animation_Timer]
Measure=Calc
Formula=[Measure_Animation_Timer]+1
IfAboveValue=1000
IfAboveAction=[!PauseMeasure "Measure_Animation_Timer"]
DynamicVariables=1
UpdateDivider=1

[MeasureFadingGUI]
Measure=Calc
Formula=[Measure_Animation_Timer]*5
IfAboveValue=300
IfAboveAction=[!PauseMeasure "MeasureFadingGUI"]
DynamicVariables=1
UpdateDivider=1

;This measure shows the warning stripes
[MeasureWarningStripesActivate]
Measure=Calc
Formula=[Measure_Animation_Timer]
IfAboveValue=100
IfAboveAction=[!CommandMeasure MeasureWarningStripes "Execute 1"]
DynamicVariables=1
UpdateDivider=1

;--------------------------PC Stats measures for Terminal Control----------------------------
[measureCPU]
; This measure returns the average CPU load between all cores.
Measure=CPU
Processor=0

[measureRAM]
; Returns the amount of RAM used in bytes.
Measure=PhysicalMemory
UpdateDivider=20
; UpdateDivider sets the rate at which the value of the measure is updated. It is
; calculated as follows: UpdateDivider x Update. In this case, Update is set to
; 1000 milliseconds (in the [Rainmeter] section). 1000 x 20 = 20000 ms or 20 seconds.

[measureSWAP]
; Returns the amount of SWAP (pagefile) used in bytes.
Measure=SwapMemory
UpdateDivider=20


;--------------------------TIME MEASURES: CREDIT TO ILLUSTRO 'CLOCK' SKIN--------------------
[measureTime]
; This measure returns the time in a 24-hour format (i.e. HH:MM).
Measure=Time
Format=%H:%M
; For a 12-hour clock, change the Format option above to: %I:%M %p
; Refer to the Rainmeter manual for other format codes.

[measureDate]
; Returns the date as DD.MM.YYYY
Measure=Time
Format=%d.%m.%Y

[measureDay]
; Returns the current day
Measure=Time
Format=%A

