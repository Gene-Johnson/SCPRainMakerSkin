;=============================================================================;
;----------------------------- Twitch chat meter -----------------------------;
;=============================================================================;

[TwitchChatTextMeter]
Meter=String
MeasureName=TwitchMain
X=(0.28 * #WORKAREAWIDTH#)
Y=(0.222222222 * #WORKAREAHEIGHT#)+310
FontFace=Consolas
FontSize=15
FontColor=255,0,0,255
AntiAlias=1
Y=#ChannelHeight#
W=2000
H=#ChatHeight#
InlineSetting=Color|#ChatUserFontColor#
InlinePattern=^ *<(.+?)>:|\n *<(.+?)>:
InlineSetting1=Color|00FF00
InlinePattern1=^ *<(#Username#)>:|\n *<(#Username#)>:
InlineSetting2=Color|00FF00
InlinePattern2=#Username#
Text=%1
DynamicVariables=1
UpdateDivider=1
@IncludeGenerated=#@#generated\TwitchGenerated.inc
