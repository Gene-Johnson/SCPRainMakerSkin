;=============================================================================;
; NOTE:                                                                       ;
; Most settings can be changed in the Variables.inc file.                     ;
; Right click the skin and select "Open settings".                            ;
; One value that can only be changed here is the Update value. Changing this  ;
; can increase performance.                                                   ;
; Another important aspect in increasing performance is to use fewer images   ;
; and links.                                                                  ;
;=============================================================================;

[Rainmeter]
Update=10
DefaultUpdateDivider=20
AccurateText=1
ContextTitle="Open user settings"
ContextAction=["#@#UserSettings.inc"]
ContextTitle2="Open settings"
ContextAction2=["#@#Variables.inc"]



[Metadata]
Name=SCP-C-COM
Author=Maximillion106
Version=1.0
Information=An SCP Based Skin from the SCP: Containment Breach games. Enjoy!
License=Creative Commons Attribution-Non-Commercial-Share Alike 3.0

[Variables]
@IncludeVariables=#@#Variables.inc
@IncludeUserSettings=#@#UserSettings.inc
UrlRegex=(https?:\/\/(?:www\.|(?!www))[^ \t\.]+\.[^\t ]{2,}|www\.[^\t ]+\.[^\t ]{2,})

;====================Variables==============

;The text value that is changed by Actiontimer-MeasureTypeWriter
TEXTVAR = ""

REDTEXTVAR=

GREENTEXTVAR=

WHITETEXTVAR=

TEXTBOOT = ""

TestText="Hello World"
;The initial wait time before starting Actiontimer-MeasureTypeWriter
TwaitInitial=500

;The wait period between typing
Twait=300

;How fast to type words
Ttype=80

;How fast to load
Tload=130

;variable used to adjust GUI fading
FadeDiffrence=0


;warning stripes fade
WarnStripesFade_1=0
WarnStripesFade_2=0
WarnStripesFade_3=0
WarnStripesFade_4=0
WarnStripesFade_5=0
