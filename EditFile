;====================General Meters==============

;--------------------SHAKING--------------
Flicker For Values
[MeasureFlicker_State_1]
Measure=Calc
Formula=Random
LowBound=10
HighBound=(#Flicker_Value# * (2/[MeasureFlicker_counter]))
UpdateRandom=1
DynamicVariables=1
UpdateDivider=1
Disabled=1

[MeasureFlicker_counter]
Measure=Calc
Formula=[MeasureFlicker_counter]+1
IfEqualValue=200
IfEqualAction=[!PauseMeasure "MeasureFlicker_State_1"][!PauseMeasure "MeasureFlicker_counter"]
DynamicVariables=1
Disabled=1

[MeasureShake_x]
Measure=Calc
Formula=Random
LowBound=10
HighBound=(#Shake_Value# * (1.5/[MeasureDecreaseValue_X]))
UpdateRandom=1
DynamicVariables=1
UpdateDivider=1
Disabled=1

This Measure produces the random variables that decide the wait time between changes in fade
[MeasureShake_y]
Measure=Calc
Formula=Random
LowBound=10
HighBound=(#Shake_Value# * (1.5/[MeasureDecreaseValue_Y]))
UpdateRandom=1
DynamicVariables=1
UpdateDivider=1
Disabled=1



